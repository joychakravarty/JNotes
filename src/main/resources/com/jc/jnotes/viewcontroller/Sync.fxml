<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="166.0" prefWidth="245.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jc.jnotes.viewcontroller.SyncController">
   <children>
      <RadioButton fx:id="newUserRadioButton" layoutX="13.0" layoutY="14.0" mnemonicParsing="false" selected="true" text="New User">
         <toggleGroup>
            <ToggleGroup fx:id="toggleGroup" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="existingUserRadioButton" layoutX="107.0" layoutY="14.0" mnemonicParsing="false" text="Existing User" toggleGroup="$toggleGroup" />
      <TextField fx:id="userIdTextField" layoutX="16.0" layoutY="44.0" prefHeight="27.0" prefWidth="215.0" promptText="Example: joy_notes" AnchorPane.bottomAnchor="95.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" />
      <TextField fx:id="userSecretTextField" layoutX="16.0" layoutY="85.0" prefHeight="27.0" prefWidth="215.0" promptText="Example: MySecret123" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" />
      <Button fx:id="connectButton" layoutX="11.0" layoutY="125.0" mnemonicParsing="false" onAction="#connectDisconnect" prefHeight="27.0" prefWidth="72.0" text="Connect" AnchorPane.leftAnchor="8.0" />
      <Button fx:id="backupButton" layoutX="87.0" layoutY="125.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="72.0" text="Backup" />
      <Button fx:id="restoreButton" layoutX="165.0" layoutY="125.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="72.0" text="Restore" AnchorPane.leftAnchor="165.0" AnchorPane.rightAnchor="8.0" />
   </children>
</AnchorPane>
